    "grammars": [
      {
        "language": "gizbox",
        "scopeName": "source.gizbox",
        "path": "./syntaxes/gizbox.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "typescript": "^5.4.5"
