{
  "name": "gizboxvscode",
  "displayName": "GizboxVSCode",
  "description": "",
  "version": "0.0.1",
  "publisher": "REW",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.89.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/REW-Z/Gizbox"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "gizbox",
